#include <iostream>
using namespace std;

int main() {
  // put your code here
    int n;
	int m;
	int arr[100][100];
	cin >> n >> m;
	int k = 1;
	//for i:
	int i_ir = 0;
	int j_ir = 0;
	int j_m_jr = 1;
	int decr = 0;
	int i_j_incr = 0;
	int k_count = 1;

	/*cout << "Previously:\n";
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			arr[i][j] = 0;
			cout << arr[i][j] << "\t";
		}
		cout << "\n";
	}
	*/
	while (k <= (n * m)){
		for (int i = i_ir, j = j_ir; j < m - decr; j++) {
			if (k <= (n * m)) {
				arr[i][j] = k++;
			}
			else {
				break;
			}
		}
		for (int i = ++i_ir, j = m - j_m_jr; i < n - decr; i++) {
			if (k <= (n * m)) {
				arr[i][j] = k++;
			}
			else {
				break;
			}
		}
		for (int i = n - i_ir, j = m - (++j_m_jr); j >= i_j_incr; j--) {		//j >= 0	
			if (k <= (n * m)) {
				arr[i][j] = k++;
			}
			else {
				break;
			}
		}
		for (int i = n - (++i_ir), j = j_ir; i > i_j_incr; i--) {				//i > 0
			if (k <= (n * m)) {
				arr[i][j] = k++;
			}
			else {
				break;
			}
		}
		i_ir--;
		j_ir++;
		decr++;
		i_j_incr++;
	}
	

	
	cout << "\n";
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			cout.width(4);
			cout << arr[i][j];
		}
		cout << "\n";
	}
  return 0;
}
